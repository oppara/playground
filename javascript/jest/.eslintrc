{
  "plugins": [
    "jest"
  ],
  "env": {
    "jest/globals": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
  },
  "rules": {
    "no-useless-computed-key": "error",
    "no-unused-vars": ["error", {
        "vars": "all",
        "args": "none"
    }],
    "comma-dangle": ["error", {
      "arrays": "only-multiline",
      "objects": "only-multiline",
      "imports": "only-multiline",
      "exports": "only-multiline",
      "functions": "ignore"
    }],
    "semi": "error"
  }
}
